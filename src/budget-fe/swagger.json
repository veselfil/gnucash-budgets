{
  "openapi": "3.0.1",
  "info": {
    "title": "GnuCashBudget.Api",
    "version": "1.0"
  },
  "paths": {
    "/expense-accounts": {
      "get": {
        "tags": [
          "ExpenseAccounts"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Account"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Account"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Account"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Account": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "commodity": {
            "type": "string",
            "nullable": true
          },
          "accountType": {
            "$ref": "#/components/schemas/AccountType"
          }
        },
        "additionalProperties": false
      },
      "AccountType": {
        "enum": [
          "root",
          "asset",
          "stock",
          "mutual",
          "bank",
          "cash",
          "mutualFund",
          "otherAssets",
          "income",
          "expense",
          "liability",
          "credit",
          "equity"
        ],
        "type": "string"
      }
    }
  }
}